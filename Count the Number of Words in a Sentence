#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    char str[2000];
    int i, count=0;
    fgets(str, 2000, stdin);
    for(i=0; str[i]!= '\0'; i++){
        if((str[i] != ' ' && str[i] != '\n') &&
            (str[i+1] == ' ' || str[i+1] == '\n' || str[i+1] == '\0')) {
            count++;
        }
    }
   
    printf("%d", count);
    return 0;
}
