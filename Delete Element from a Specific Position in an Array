#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int i, n;
    scanf("%d", &n);
    int arr[n];
    for(i=0; i<n; i++){
        scanf("%d", &arr[i]);
    }
    int position;
    scanf("%d", &position);
    if(position<1|| position>n){
        printf("Invalid position");
        return 0;
    }
    for(i=position-1; i<n-1; i++){
        arr[i]= arr[i+1];
    }
    for(i=0; i<n-1; i++){
        printf("%d ", arr[i]);
    }
    return 0;
}
