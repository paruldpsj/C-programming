#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
 
    int n;
    scanf("%d", &n);
    int a[n],b[n],c;
    int *p1, *p2;
    for(int i=0; i<n; i++){
        scanf("%d", &a[i]);
    }
    for(int i=0; i<n; i++){
        scanf("%d", &b[i]);
    }
    for(int i=0; i<n; i++){
    p1 = &a[i];
    p2 = &b[i];
    c = *p1;
    *p1 = *p2;
    *p2 = c;
    }
    for(int i = 0; i < n; i++)
        printf("%d ", a[i]);

    printf("\n");

    for(int i = 0; i < n; i++)
        printf("%d ", b[i]);
    return 0;
}
