#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int n, i;
    scanf("%d", &n);
    int arr[n], largest, smallest;
    for(i=0; i<n; i++){
        scanf("%d", &arr[i]);
    }
    largest=arr[0];
    for(i=1; i<n; i++){
        if(arr[i]>largest){
            largest= arr[i];
        }
    }
    smallest=arr[0];
    for(i=1; i<n; i++){
        if(arr[i]<smallest){
            smallest= arr[i];
        }
    }
    printf("%d %d", smallest, largest);
    return 0;
}
