#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int n, i;
    scanf("%d", &n);
    int arr[n], key, count=0;
    for(i=0; i<n; i++){
        scanf("%d", &arr[i]);
    }
    scanf("%d", &key);
    for(i=0; i<n; i++){
        if(arr[i]==key){
            count++;
        }
    }
    printf("%d", count);
    
    return 0;
}
