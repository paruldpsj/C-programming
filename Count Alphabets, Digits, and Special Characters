#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#include<ctype.h>

int main() {
    
    char str[1000];
    int i, count=0, digit=0, special=0;
    fgets(str, 1000, stdin);
    for(i=0; i<strlen(str); i++){
        if(isalpha(str[i])){
            count++;
        }
        else if(isdigit(str[i])){
            digit++;
        }
        else{
            special++;
        }
    }
    printf("%d %d %d", count, digit, special);
    return 0;
}
