#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int i, n;
    scanf("%d", &n);
    int arr[n], *ptr;
    ptr = arr;
    for(i=0; i<n; i++){
        scanf("%d", ptr+i);
    }
    for(i=0; i<n; i++){
        printf("%d ", *(ptr+i));
    }

    return 0;
}
