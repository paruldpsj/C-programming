#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    char ch;
    int x;
    float y;
    double z;
    scanf("%c", &ch);
    scanf("%d", &x);
    scanf("%f", &y);
    scanf("%lf", &z);
    printf("%c %d %.2f %.6lf", ch, x, y, z);
    return 0;
    
    
   
}
