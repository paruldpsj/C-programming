#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int r, c, i, j;
    scanf("%d %d", &r, &c);
    int arr[r][c];
    for(i=0; i<r; i++){
        for(j=0; j<c; j++){
            scanf("%d", &arr[i][j]);
        }
    }

    for(i=0; i<r; i++){
        int sum=0;
        for(j=0; j<c; j++){
            sum += arr[i][j];
           
        }
         printf("%d\n", sum);
    }
        

        for(j=0; j<c; j++){
            int sum=0;
             for(i=0; i<r; i++){
            sum += arr[i][j];
           
        }
        printf("%d\n", sum); 
         
    }
            
    return 0;
}
