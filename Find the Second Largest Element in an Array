#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int n;
    scanf("%d", &n);
    int arr[n];
    for(int i=0; i<n; i++){
        scanf("%d", &arr[i]);
    }
    int largest = arr[0], large = arr[0];
    for(int i=1; i<n; i++){
        if(arr[i]>largest)
            largest= arr[i];
    }
        int found= 0;
        for(int i=1; i<n; i++){
            if(arr[i]!= largest){
                if(!found || arr[i]>large){
                    large = arr[i];
                found = 1;
            }
        }
    }
    
    if(found)
        printf("%d", large);
    else
        printf("No second largest element");
    
    return 0;
}
