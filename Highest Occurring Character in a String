#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

   char str[1000];
    int i, j, max=0;
    fgets(str, 1000, stdin);
    int index=-1;
    for(i=0; i<strlen(str); i++){
        int count = 0;
       for(j=0; j<strlen(str); j++){
           if(str[i]==str[j]){
               count++;
           }
       }
       
        if(count>max){
            max= count;
            index=i;
        }
    }
    printf("%c occurs %d times", str[index], max);
    return 0;
}
