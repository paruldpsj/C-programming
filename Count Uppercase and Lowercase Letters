#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    char str[1000];
    int i, count=0, con=0;
    fgets(str, 1000, stdin);
    for(i=0; i<strlen(str); i++){
        if(isalpha(str[i])){
            if(isupper(str[i])){
                count++;  
            }
            else{
                con++;
            }
        }
    }
    printf("%d %d", count, con);
    return 0;
}
