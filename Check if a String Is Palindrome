#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    char str[1000];
    int i, found=0;
    fgets(str, 1000, stdin);
    for(i=0; i<(strlen(str))/2; i++){
        if(tolower(str[i])!=tolower(str[(strlen(str)-1)-i])){
            found=1;
        }
    }
    if(found==1){
            printf("Not Palindrome");
        }
        else{
            printf("Palindrome");
        }
    return 0;
}
