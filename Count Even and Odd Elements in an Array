#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n, i;
    scanf("%d", &n);
    int arr[n];
    for(i=0; i<n; i++){
        scanf("%d", &arr[i]);
    }
    int odd=0, even=0;
    for(i=0; i<n; i++){
        if(arr[i]%2!=0){
            odd++;
        }
        else
            even++;
    }
    printf("%d %d", even, odd);

        
    return 0;
}
